#%Module1.0#####################################################################
##
## ffmpeg 2.8.3 modulefile
##

## Module file logging
source /usr/local/etc/module_logging.tcl
##

module load compilers/gcc/5.2


proc ModulesHelp { } {
        puts stderr "Makes ffmpeg 2.8.3 available"
}

set FFMPEG_DIR /usr/local/packages6/apps/gcc/5.2/ffmpeg/2.8.3

module-whatis   "Makes ffmpeg 2.8.3 available"

prepend-path PATH $FFMPEG_DIR/bin
prepend-path LD_LIBRARY_PATH $FFMPEG_DIR/lib
prepend-path LIBRARY_PATH $FFMPEG_DIR/lib
prepend-path CPATH $FFMPEG_DIR/include
prepend-path MANPATH $FFMPEG_DIR/share/man
