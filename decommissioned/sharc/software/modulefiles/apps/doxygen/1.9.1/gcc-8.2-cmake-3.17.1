#%Module1.0#####################################################################
##
## doxygen 1.9.1 module file
##

## Module file logging
source /usr/local/etc/module_logging.tcl
##

proc ModulesHelp { } {
        puts stderr "Makes doxygen 1.9.1 available"
}

module-whatis   "Makes doxygen 1.9.1 available"

# Add required module loads

module load dev/gcc/8.2
module load dev/cmake/3.17.1/gcc-8.2

# Set package root directory
set ROOT_DIR_doxygen  /usr/local/packages/apps/doxygen/1.9.1/gcc-8.2-cmake-3.17.1/


# Set executable path
prepend-path PATH        		 $ROOT_DIR_doxygen/bin


# Set CMAKE PREFIX PATH
prepend-path CMAKE_PREFIX_PATH 	 $ROOT_DIR_doxygen

