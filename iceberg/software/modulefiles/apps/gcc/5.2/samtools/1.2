#%Module1.0#####################################################################
##
## Samtools 1.2 modulefile
##

## Module file logging
source /usr/local/etc/module_logging.tcl
##

module load compilers/gcc/5.2


proc ModulesHelp { } {
        puts stderr "Makes Samtools 1.2 available"
}

set version 1.2
set SAM_DIR /usr/local/packages6/apps/gcc/5.2/samtools/$version

module-whatis   "Makes samtools 1.2 available"

prepend-path PATH $SAM_DIR/bin
prepend-path MANPATH $SAM_DIR/share/man/
