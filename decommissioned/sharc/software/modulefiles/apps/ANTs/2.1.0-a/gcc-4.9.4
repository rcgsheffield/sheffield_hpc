#%Module1.0#####################################################################
##
## ANTs module file
##

## Module file logging
source /usr/local/etc/module_logging.tcl
##

proc ModulesHelp { } {
        puts stderr "Adds ANTs 2.1.0-a to your environment"
	puts stderr "and loads compiler gcc/4.9.4"
}

module-whatis   "Makes ANTs 2.1.0-a available"

if { [ module-info mode load ] } {
    puts stderr "NOTE: the ANTs module also loads the gcc-4.9.4 compiler"
}

module load dev/gcc/4.9.4

prepend-path PATH /usr/local/packages/apps/ANTs/2.1.0-a/gcc-4.9.4/bin

setenv ANTSPATH /usr/local/packages/apps/ANTs/2.1.0-a/gcc-4.9.4/bin
