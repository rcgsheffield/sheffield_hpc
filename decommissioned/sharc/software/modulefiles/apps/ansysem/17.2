## Module file logging
source /usr/local/etc/module_logging.tcl
##

proc ModulesHelp { } {
        global version

        puts stderr "   Makes ANSYS EM Version 17.2 available for use"
}

module-whatis   "Makes ANSYS EM V17.2 available"

# load modules
  module load apps/ansys/17.2

# module variables
#
 set ANSYSEM_DIR     /usr/local/packages/apps/ansysem/17.2/AnsysEM17.2


prepend-path PATH $ANSYSEM_DIR/Linux64

prepend-path LD_LIBRARY_PATH /usr/local/packages/apps/ansysem/17.2/AnsysEM17.2/Linux64