## Module file logging
source /usr/local/etc/module_logging.tcl
##

proc ModulesHelp { } {
        global version

        puts stderr "   Makes ANSYS EM Version 16.1 available for use"
}

module-whatis   "Makes ANSYS EM V16.1 available"

# load modules
 module load apps/ansys/16.1

# module variables
#
 set ANSYSEM_DIR     /usr/local/packages/apps/ansysem/16.1/AnsysEM16.1


prepend-path PATH $ANSYSEM_DIR/Linux64

prepend-path LD_LIBRARY_PATH /usr/local/packages/apps/ansysem/16.1/AnsysEM16.1/Linux64