#%Module1.0#####################################################################
##
## ctffind 4.1.5 module file
##

## Module file logging
source /usr/local/etc/module_logging.tcl

set ctffindvers 4.1.5
set compiler gcc
set compilervers 4.9.2

proc ModulesHelp { } {
    global ctffindvers 
    puts stderr "Adds ctffind $ctffindver to your environment"
}
module-whatis "Adds ctffind $ctffindvers your environment"

module load compilers/${compiler}/${compilervers}
module load libs/${compiler}/${compilervers}/wxWidgets/3.1.0
module load libs/intel-mkl/11.2.3

set ctffindroot /usr/local/packages6/apps/$compiler/$compilervers/ctffind/$ctffindvers

prepend-path    PATH             $ctffindroot/bin
setenv          TMPDIR           /scratch/
