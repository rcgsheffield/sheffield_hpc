#%Module1.0#####################################################################
#
# libmboard 0.3.1 (used by FLAME) module file
#
# Will Furnass 2018
################################################################################

# Module file logging
source /usr/local/etc/module_logging.tcl

proc ModulesHelp { } {
        global vers

        puts stderr "   Makes libmboard $vers (used by FLAME) available."
}

set vers 0.3.1
set prefix /usr/local/packages/libs/libmboard/$vers/gcc-6.2

module-whatis "Makes libmboard $vers (used by FLAME) available."

prepend-path PATH $prefix/bin
prepend-path CPATH $prefix/include
prepend-path LD_LIBRARY_PATH $prefix/lib
prepend-path LIBRARY_PATH $prefix/lib
