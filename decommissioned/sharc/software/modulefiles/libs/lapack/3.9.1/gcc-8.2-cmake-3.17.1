#%Module1.0#####################################################################
##
## lapack 3.9.1 module file
##

## Module file logging
source /usr/local/etc/module_logging.tcl
##

proc ModulesHelp { } {
        puts stderr "Makes lapack 3.9.1 available"
}

module-whatis   "Makes lapack 3.9.1 available"

# Add required module loads

module load dev/gcc/8.2
module load dev/cmake/3.17.1/gcc-8.2

# Set package root directory
set ROOT_DIR_lapack  /usr/local/packages/libs/lapack/3.9.1/gcc-8.2-cmake-3.17.1/


# Set 64 bit library paths
prepend-path LD_LIBRARY_PATH 	 $ROOT_DIR_lapack/lib64
prepend-path LIBRARY_PATH 	     $ROOT_DIR_lapack/lib64


# Set CMAKE PREFIX PATH
prepend-path CMAKE_PREFIX_PATH 	 $ROOT_DIR_lapack


# Set 64bit PKG_CONFIG_PATH
prepend-path PKG_CONFIG_PATH     $ROOT_DIR_lapack/lib64/pkgconfig

